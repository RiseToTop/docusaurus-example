{
  "redirects": [
    {
      "source": "/",
      "destination": "/docs",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/docs",
      "destination": "https://docusaurus-example-three.vercel.app"
    },
    {
      "source": "/docs/(.*)",
      "destination": "https://docusaurus-example-three.vercel.app/$1"
    }
  
  ]
}
